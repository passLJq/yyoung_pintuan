page {
  background-color: #f5f5f5;
}

.msitem {
  display: flex;
  align-items: center;
  background-color: #fff;
}

.msitem image {
  width: 96rpx;
  height: 96rpx;
  margin-right: 30rpx;
  flex-shrink: 0;
}

.box {
  /* background-color: #fff; */
  padding: 30rpx;
}

.out {
  justify-content: space-between;
  background: #fff;
  padding: 0 0 0 25rpx;
  height: 144rpx;
  /* margin: 0 30rpx; */
  border-bottom: 1px solid #f6f6fa;
  border-radius: 8rpx;
  overflow: hidden;
}
.out + .out {
  margin-top: 20rpx;
}

.titles {
  max-width: 370rpx;
}

.titles view:nth-child(1) {
  font-size: 34rpx;
  font-weight: 500;
  color: #2C5B33;
}

.titles view:nth-child(2) {
  font-size: 28rpx;
  color: #666464;
  margin-top: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.img_box {
  position: relative;
}

.icon_corner {
  position: absolute;
  top: 0;
  right: 0;
  transform: translateX(-50%);
  width: 22px;
  height: 14px;
  background: #EB6100;
  border-radius: 7px;
  font-size: 12px;
  text-align: center;
  line-height: 14px;
  color: #fff;
  box-sizing: border-box;
}

.right {
  width: 140rpx;
  height: 100%;
  background-color: #F5AC9E;
  display: flex;
  justify-content: center;
  align-items: center;
}

.right text {
  color: #fff;
  font-size: 46rpx;
}