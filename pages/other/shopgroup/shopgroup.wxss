page{overflow: hidden;}
.box_img_container{position: absolute; top: 100rpx; margin-left: 72rpx; width: 610rpx; height: 796rpx; background-color: #fff; border-radius: 16rpx;}
.box_content_container{position: absolute; top:950rpx;width: 100%; text-align: center;color: #fff; font-size: 28rpx;}
