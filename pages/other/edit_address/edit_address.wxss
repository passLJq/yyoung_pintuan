/* pages/other/edit_address/edit_address.wxss */
page{background-color: #fff;}
.list_container{height: 96rpx; padding: 0 32rpx; line-height:  96rpx; color: #333; font-size: 28rpx; box-sizing: border-box;}
.list_container>view,.detail_address_box{border-bottom: 2rpx solid #F6F6FA; }
.detail_address_box{color: #999; font-size: 28rpx; padding: 20rpx 32rpx;}
.list_title{width: 20%;}
.footer_container{position: fixed; bottom:88rpx; width: 100%; background-color: #fff;}
.footer_container>button{border-radius: 44rpx; margin-top: 24rpx; height: 88rpx; line-height: 88rpx;}
.cancel_btn{display:block;margin-top:24rpx;height:88rpx;line-height:88rpx;background: #fff; border: 2rpx solid #AFAFAF;border-radius: 44rpx; text-align: center;}