@import "../../common/iconfont.wxss";
@import "../../common/font-awesome.wxss";
@import "../../Component/sharebox/sharebox.wxss";

view {
  box-sizing: border-box;
}
/* 分享按钮 */
.share {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  position: absolute;
  background-color: #d8d8d8;
  top: 30rpx;
  right: 30rpx;
  z-index: 999;
  text-align: center;
  line-height: 72rpx;
  font-size: 60rpx;
  color: #fff;
}

/* 轮播图区域 */
.swiper {
  width: 750rpx;
  height: 750rpx;
  background-color: #ccc;
  position: relative;
}
.swpier swiper {
  width: 750rpx;
  height: 750rpx;
}

.swpier .swiper-item {
  width: 100%;
  height: 100%;
}

.swiper .slide-image {
  width: 100%;
  height: 100%;
}
/* 轮播指示 */
.page {
  padding: 0 20rpx;
  height: 34rpx;
  background-color: rgba(51, 51, 51, .6);
  position: absolute;
  right: 32rpx;
  bottom: 128rpx;
  border-radius: 17rpx;
}
.page text {
  font-size: 20rpx;
  color: #fff;
}
/* 价格区域 */
.priceGroup {
  width: 750rpx;
  height: 96rpx;
  color: #fff;
  padding-left: 22rpx;
  /* padding-top: 16rpx; */
  position: absolute;
  left: 0;
  bottom: 0;
  background-color: #427161; 
  display: flex;
  align-items: center;
}
.priceGroup .price {
  font-size: 60rpx;
  margin-right: 8rpx;
  line-height: 60rpx;
}
.cjj {
  font-size: 24rpx;
  padding: 5rpx 20rpx;
  background-color: #EB6100;
  border-radius: 30rpx;
}
.np {
  color: #E69888;
  font-size: 40rpx;
  margin-left: 20rpx;
}
.olddprice {
  font-size: 26rpx;
  color: #B2B2B2;
  text-decoration:line-through;
  margin-left: 20rpx;
}
.priceGroup .oldPrice {
  display: inline-block;
  position: absolute;
  min-width: 106rpx;
  height: 100%;
  top: 48%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
}
.priceGroup .oldPrice text{
  font-size: 20rpx;
}
.priceGroup .oldPrice > view {
  position: absolute;
  /* width:80rpx; */
  height:28rpx;
  background:rgba(255,255,255,1);
  border-radius:16rpx;
  color: #FF5541;
  font-size: 20rpx;
  text-align: center;
  line-height: 28rpx;
  margin-top: 8rpx;
  padding: 0 10rpx;
}

/* 商品信息 */
.proDetails {
  width: 100%;
  padding: 0 65rpx;
  background-color: #fff;
  justify-content: space-between;
}
.proDetails .details {
  width: 100%;
  line-height: 6rpx;
  margin-top: 16rpx;
  font-size:32px;
  color: #666464;
}
.slogan {
  font-size: 20rpx;
  display: inline-block;
  height: 32rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius:16px;
  border: 2rpx solid #EB6100; 
  color: #EB6100;
  margin-bottom: 44rpx;
  /* transform: translateY(-2rpx);
  -webkit-transform: translateY(-2rpx); */
}
.slogan text {
  font-size: 20rpx;
  line-height: 30rpx;
  padding: 0 12rpx;
  display: inline-block;
  color: #fff;
  height: 100%;
  /* margin-right: 10rpx; */
  border-radius: 16rpx;
}
.slogan .buy {
  margin: 0 10rpx;
  color: #EB6100;
  padding: 0;
}
.proDetails .details text {
  /* position: absolute; */
  font-size: 28rpx;
  line-height: 48rpx;
}

/* 保障 */
.ensure {
  width: 750rpx;
  height: 70rpx;
  background-color: rgba(255,255,255,0.6);
}
.ensure view {
  width: 33.333%;
  height: 100%;
}
.ensure view image {
  width: 28rpx;
  height: 28rpx;
  display: inline-block;
}
.ensure view text {
  font-size: 24rpx;
  color: #666464;
}
/* 玩法说明 */
.explain {
  margin-top: 20rpx;
  width: 750rpx;
  height: 270rpx;
  background-color: #fff;
}
.title {
  text-align: center;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #333;
}
.explain .content {
  width: 750rpx;
  height: 200rpx;
  padding-top: 34rpx;
  /* padding-left: 60rpx; */
}
.explain .content .progress{
  display: block;
  width: 100%;
  height: 44rpx;
}
.explain .content .progress image{
  /* width: 100%;
  height: 100%; */
  width: 470rpx;
  height: 40rpx;
  margin: 0 auto;
}
.explain .content .content-item {
  width: 96rpx;
  font-size: 24rpx;
  color: #9B9B9B;
  line-height: 34rpx;
  margin-top: 18rpx;
  margin-left: 110rpx;
  display: inline-block;
}
.explain .content .content-item + .content-item {
  margin-left: 48rpx;
}
/* 拼团成员 */
.member {
  width: 750rpx;
  background-color: #fff;
  padding: 0 40rpx;
  margin-top: 20rpx;
	/* max-height: 700rpx; */
	/* overflow-y: scroll; */
}
::-webkit-scrollbar {
	display: none;
}
.member .member-item {
  width: 100%;
  height: 140rpx;
  align-items: center;
}
.member .member-item + .member-item {
  border-top: 1rpx solid #f6f6f6;
}
.member .member-item image {
  width: 96rpx;
  height: 96rpx;
  border-radius:50%;
  -webkit-border-radius:50%;
  margin-right: 20rpx;
}
.member .member-item view {
  height: 86rpx;
}
.member .member-item view text {
  font-size: 28rpx;
  max-width: 390rpx;
  color: #427161;
}
.member .member-item view text:nth-child(2) {
  font-size: 24rpx;
  color: #9B9B9B
}
.member .member-item .btn {
  width: 160rpx;
  height: 60rpx;
  border-radius: 60rpx;
  -webkit-border-radius: 60rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
}

/* 拼团弹窗 */
.ptPop {
  width: 570rpx;
  border-radius: 16rpx;
  background-color: #fff;
  position: fixed;
  left: 90rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 1001;
}
.ptPop > text {
  font-size: 36rpx;
  font-weight: 600;
  color: #666464;
  margin-top: 48rpx;
}
.ptPop > .avatar {
  justify-content: space-around;
  width: 50%;
  margin: 30rpx 0;
}
.ptPop > .avatar image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}
.ptPop > .avatar .tuanzhang {
  width: 50rpx;
  height: 32rpx;
  font-size: 20rpx;
  color: #fff;
  position: absolute;
  left: 0;
  top:20%;
  background-color: #F5AC9E;
  /* bottom: -16rpx; */
  transform: translateX(-50%);
  border-radius: 16rpx;
}
.ptPop > .btn {
  width: 480rpx;
  height: 72rpx;
  border-radius: 36rpx;
  color: #fff;
  font-size: 28rpx;
  font-weight: 600;
  background-color: #F5AC9E;
  margin-bottom: 20rpx;
}

/* 商品详情 */
.productMsg {
  width: 750rpx;
  margin-top: 20rpx;
  background-color: #fff;
  padding-bottom: 110rpx;
}
.productMsg .content {
  width: 100%;
}
.productMsg .content image {
  width: 100%;
}

/* 底部栏 */
.footerBar {
  width: 100%;
  height: 110rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  z-index: 99;
  box-sizing: content-box;
	display: flex;
	justify-content: center;
	align-items: center;
}
.footerBar .iconView {
  width: 98rpx;
  height: 98rpx;
  margin-top: 6rpx;
  
}
.iconView text:nth-child(1) {
  font-size: 45rpx;
  margin-top: 12rpx;
}
.iconView text:nth-child(2) {
  font-size: 20rpx;
  margin-top: 10rpx;
}
.footerBar .buyNow {
  width: 370rpx;
  height: 70rpx;
  /* margin-top: 20rpx; */
  /* margin-left: 30rpx; */
  border: 1px solid #F5AC9E;
  -webkit-border-radius: 35px;
  border-radius: 35px;
}
.footerBar .buyNow text {
  font-size: 28rpx;
  font-weight: 600;
  color: #F5AC9E;
}
.footerBar .ptBuy {
  width: 370rpx;
  height: 70rpx;
  -webkit-border-radius: 35px;
  border-radius: 35px;
  margin: 0 0 0 10rpx;
}
.footerBar .ptBuy text {
  color: #fff;
  font-size: 28rpx;
  font-weight: 600;
}

/* 保障说明弹窗 */
.popup {
  position: relative;
}

.popup-content {
  width: 100%;
  position: fixed;
  left: 0;
  z-index: 999;
  background-color: #f2f2f2;
  /* border-radius: 30rpx 30rpx 0 0;  */
}
.popup-content .popup-title {
  width: 100%;
  height: 104rpx;
  line-height: 104rpx;
}
.popup-content .popup-item {
  padding: 20rpx 40rpx;
}
.popup-content .popup-item view {
  font-size: 34rpx;
  margin-bottom: 22rpx;
  color: #333;
}
.popup-content .popup-item view image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 16rpx;
}
.popup-content .popup-item .popup-text {
  font-size: 24rpx;
